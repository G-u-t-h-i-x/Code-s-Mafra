public class main_class {
 public static void main(String[] args) {
        int n01, n02;
        for (n01=1;n01 <= 10; n01 ++){
            System.out.println("\nTabuada de Soma "+n01);
            for (n02 = 1; n02 <= 10; n02++) {
                System.out.print(n01+" + "+n02+" = "+(n01+n02)+"  ->  ");
            }   
        }
    }
 }
